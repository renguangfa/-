<!-- pages/help-center/help-center.wxml -->
<!-- 租借指南  归还指南 -->
<view class="zhinan">
    <view class="zhinan_btn" style="background-color: #4CA462;" bindtap="zujie">租借指南</view>
    <view class="zhinan_btn" style="background-color: #4CA8E2;" bindtap="guihuan">归还指南</view>
</view>
<!-- 内容 -->
<view class="contet">
    <view class="title">常见问题</view>
    <view class="weui-cells weui-cells_after-title">
        <!-- 问题标题1111111111 -->
        <navigator bindtap="tigger" data-num="1" url="" class="weui-cell weui-cell_access" hover-class="none" style="height:80rpx;">
            <view class="weui-cell__hd">
                <image src="/assets/images/shandian.png" style="margin-right: 10rpx;vertical-align: middle;width:30rpx; height: 30rpx;"></image>
            </view>
            <view class="weui-cell__bd">充电宝归还后仍在计费</view>
            <view class="weui-cell__ft">
                <image class="right_icon" class="down {{ num == 1 ? 'rotateUp' : ''}}" src="/assets/images/down_hei.png" mode="aspectFit"></image>
            </view>
        </navigator>
        <!-- 隐藏的内容 1111111111111 -->
        <view class="classname {{num == 1?'':'hidden'}}">
            请回到小程序首页查看附近可归还地点，前去有仓位的机位地点归还哦，感谢您的使用！
        </view>

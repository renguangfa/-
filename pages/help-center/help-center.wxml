<view class="button-sp-area">
    <button class="weui-btn btnbottom" plain="true">联系客服</button>
</view>
<!-- 租借指南  图片 -->
<view class="zujie {{flag?'displayblock':'displaynone'}}" bindtap="zujieimg">
    <image class="img_img" src="/assets/images/zujie.png"></image>
</view>
<!-- 归还指南  图片 -->
<view class="guihuan {{flag1?'displayblock1':'displaynone1'}}" bindtap="guihuanimg">

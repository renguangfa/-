.bottom {
    position: fixed;
    width: 100%;
    bottom: 0rpx;
    border-top: 2rpx solid #000;
    /* padding: 20rpx; */
}

.bottom_left {
    float: left;
    padding-left: 40rpx;
    width: 65%;
}

.bottom_left_line1 {
    font-size: 30rpx;
}

.bottom_left_line2 {
    font-size: 24rpx;
    color: #575757;
}

.bottom_right {
    float: right;
}

.yuan {
    width: 100rpx;
    height: 100rpx;
    border: 2rpx solid #000;
    border-radius: 50%;
    margin: 0 auto;
    text-align: center;
    line-height: 100rpx;
}